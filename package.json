{
  "name": "cypress-mochawesome-docker",
  "version": "1.0.0",
  "description": "# UI Automation Framework",
  "main": "index.js",
  "dependencies": {
    "cypress-commands": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.10.3",
    "mocha": "^7.1.2",
    "mochawesome": "^6.1.0",
    "mochawesome-merge": "^4.0.3"
  },
  "devDependencies": {
    "cypress": "^5.5.0"
  },
  "scripts": {
    "start": "cypress open",
    "test": "cypress run test --reporter mochawesome",
    "record": "cypress run test --record",
    "cy:sanity:prod:electron": "yarn run test --browser electron",
    "cy:sanity:prod:chrome": "yarn run test --browser chrome",
    "cy:sanity:prod:chrome:headless": "yarn run test --browser chrome --headless",
    "cy:sanity:prod:ff": "yarn run test --browser firefox",
    "cy:sanity:prod:edge": "yarn run test --browser edge"
  },
